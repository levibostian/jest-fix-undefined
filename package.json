{
  "name": "jest-fix-undefined",
  "version": "0.1.2",
  "description": "References for Jest that are not packaged by default to solve the, `ReferenceError: Request is not defined` error. ",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "test": "NODE_ENV=test npx jest --testPathPattern=\".+test/.+.spec.ts\"",
    "commit": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levibostian/jest-fix-undefined.git"
  },
  "keywords": [
    "jest"
  ],
  "author": "Levi Bostian <levi.bostian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/levibostian/jest-fix-undefined/issues"
  },
  "homepage": "https://github.com/levibostian/jest-fix-undefined#readme",
  "husky": {
    "hooks": {
      "commit-msg": "echo 'Linting your commit message...(run `npm run commit` to make your life easier)'; npx commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/travis-cli": "^8.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/npm": "^5.1.15",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "commitizen": "^4.0.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
